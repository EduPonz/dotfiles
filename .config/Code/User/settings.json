        "javascript",
        "javascriptreact",
        "json",
        "jsonc",
        "latex",
        "less",
        "markdown",
        "php",
        "plaintext",
        "pug",
        "python",
        "restructuredtext",
        "rust",
        "scala",
        "scss",
        "text",
        "typescript",
        "typescriptreact",
        "xml",
        "yaml",
        "yml"
    ],
    "cSpell.ignoreWords": [
        "DBUILD",
        "DDLL",
        "DDOXYGEN",
        "DINSTALLER",
        "DMANIFEST",
        "DMSVCARCH",
        "DOXYGEN",
        "DTHIRDPARTY",
        "installercop",
        "pandocfilters",
        "plantuml",
        "puml",
        "retcode",
        "vcvarsall"
    ],
    "cSpell.maxNumberOfProblems": 10000,
    "cSpell.userWords": [
        "agregate",
        "asus",
        "auxiliar",
        "behaviour",
        "clearpath",
        "cmake",
        "colcon",
        "configurator",
        "ctest",
        "data",
        "datagram",
        "datagrams",
        "dcmake",
        "debbuger",
        "deserialization",
        "eduponz",
        "enlazar",
        "eprosima",
        "evaluable",
        "executables",
        "fastdds",
        "fastddsgen",
        "fastrtps",
        "fastrtpsgen",
        "from",
        "generator",
        "gnss",
        "GPGGA",
        "levelno",
        "lifecycles",
        "lines",
        "mantenimiento",
        "metapackages",
        "metas",
        "multicast",
        "nargs",
        "nmea",
        "organigrams",
        "paramiko",
        "platform",
        "Ponz",
        "Popen",
        "pos",
        "preliminar",
