{
    "[c++]": {
        "editor.rulers": [
            120
        ]
    },
    "[git-commit]": {
        "editor.rulers": [
            50
        ]
    },
    "[python]": {
        "editor.formatOnType": true,
        "editor.rulers": [
            79
        ]
    },
    "[shellscript]": {
        "editor.rulers": [
            100
        ]
    },
    "arduino.useArduinoCli": true,
    "cmake.configureOnOpen": false,
    "cSpell.checkLimit": 10000,
    "cSpell.enabledLanguageIds": [
        "asciidoc",
        "c",
        "cpp",
        "csharp",
        "css",
        "git-commit",
        "go",
        "handlebars",
        "haskell",
        "html",
